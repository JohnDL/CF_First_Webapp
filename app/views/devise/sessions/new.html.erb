<div class="container page-wrap">		
	<div class="content-container">
		<div class="row">
			<div class="header">
				<h1>Welcome Back!</h1>
			</div>
			</br>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

  <div class="field">
    <div class="col-md-8">
    <%= f.label(:email, class: "control-label", for: "inputEmail3") %>
    <%= f.email_field(:email, autofocus: true, class: "signup-form-control", placeholder: "Enter Email", id: "inputEmail3") %>
  </br>
  </br>

    <%= f.label :password %>
    <%= f.password_field(:password, autocomplete: "off", class: "signup-form-control", placeholder: "Enter Password", id: "inputPassword3") %>
  </br>
  </br>


  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in", class: "btn btn-default" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>

</div>
</div>
</div>
</div>
</div>
