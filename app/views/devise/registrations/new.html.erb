<div class="container page-wrap">		
	<div class="content-container">
		<div class="row">
			<div class="header">
				<h1>Sign up for an acount</h1>
			</div>
			</br>
  

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  
  <div class="field">
    <div class="col-md-8">
      <%= f.label :first_name %>
      <%= f.text_field(:first_name, class: "signup-form-control", placeholder: "Enter first name") %>
    
    </br>
    </br>

  <div class="field">
      <%= f.label :last_name %>
      <%= f.text_field(:last_name, class: "signup-form-control", placeholder: "Enter last name") %>
  
    </br>
    </br>

  <div class="field">
    <%= f.label(:email, class: "control-label", for: "inputEmail3") %>
    <%= f.email_field(:email, autofocus: true, class: "signup-form-control", placeholder: "Enter Email", id: "inputEmail3") %>
    
    
    </br>
    </br>
    
    <%= f.label :password %>
    <%= f.password_field(:password, autocomplete: "off", class: "signup-form-control", placeholder: "Enter Password", id: "inputPassword3") %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %>
    
    </br>
    </br>
    
    <%= f.label(:password_confirmation, class: "control-label", for: "inputPassword3") %>
    <%= f.password_field(:password_confirmation, autocomplete: "off", class: "signup-form-control", placeholder: "Enter Password", id: "inputPassword3") %>
    
    </br>
    </br>

  <div class="actions">
    <%= f.submit "Sign up", class: "btn btn-default" %>
    
  </div>
<% end %>

<%= render "devise/shared/links" %>

</div>
</div>
</div>
</div>
</div>
